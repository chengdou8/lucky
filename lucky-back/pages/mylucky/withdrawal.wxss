.lucky-title {
  overflow:auto;
}
.mylucky-title,.mylucky-sex {
  float: left;
}
.mylucky-sex image {
  float: left;
  width: 18px;
  height: 18px;
  margin-left: 10px;
  margin-top: 5px;
}
.cash-form {
  background-color: #fff;
  padding: 20px 10%;
  border-top:1px solid #efefef;
  border-bottom:1px solid #efefef;
  margin-top: 10px;
}
.cash-row {
  font-size: .725rem;
  padding: 5px 0;
}
wx-input.cash-input {
  width: 100%;
  padding: 2px 8px;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box; 
  -o-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  border-radius: 5px;
  margin-top: 5px;
  height: auto !important;
  box-shadow: 0 0 2px rgb(153,153,153);
}
.cash-btn {
  width:100%;
  padding:0;
  margin-top:10px;
  font-size: .875rem;
  line-height: 2.35;
}
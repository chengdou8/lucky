.mark-row {
  background-color: #fff;
  padding:10px;
}
.mark-row .btn {
  float: right;
  padding: 2px 10px;
  font-size: .625rem;
  margin-top: 3px;
}
.mark-row .branch {
  display: inline-block;
  font-size: .625rem;
  padding: 2px 0 0 5px;
  vertical-align: middle;
  border:none;
}
.bottom-fixed .movebox {
  float: left;
  width: 75%;
}
.wx-slider-track {
  background: #fea758;
  border-color: #fea758;
}
div.wx-slider-step {
  background: #fea758;
}
.topImg image {
  width:100%;
  height:92px;
  max-height:92px;
  -webkit-box-shadow:0 2px 2px #CCC;
  box-shadow:0 2px 2px #CCC;
}
.wx-slider-handle {
  width: 24px !important;
  height: 24px !important;
  background: #fea758 !important; 
}
slider {
  margin: 10px 25px 10px 18px;
}
.btn.btn-yellow {
  position: relative;
}
.btn.btn-yellow .bin {
  position:absolute;
  left:-24px;
  color:#888;
  font-size:13px;
  top:-1px;
}
.dist {
  margin-bottom: 40px;
}
.dist .distItem:first-child {
  margin-top: 5px;
}
.distItem {
  padding: 10px 15px;
  background-color: #fff;
}
.dist .distItem:last-child  {
  border-bottom-width: 0;
  margin-bottom: 5px;
}
.disRow {
  font-size: .75rem;
  line-height: 1.5;
}
.imgList {
  display: flex;
  flex-direction: row;
}
.imgList .imgItem {
  display: inline-flex;
  height: auto;
  flex-grow: 1;
  padding: 5px 0;
}
.imgList .imgItem:nth-child(2),.imgList .imgItem:nth-child(3),.imgList .imgItem:nth-child(4) {
  padding-left: 2%;
}
.imgItem image {
  width: 100%;
  height: 105px;
}
.disTime {
  font-size:.625rem;
  color:#959595;
  padding-top:5px;
}
.pointdes {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;      
  -webkit-box-orient: vertical;
  padding-bottom: 5px;
}